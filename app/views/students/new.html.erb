<!-- Content Header (Page header) -->
<section class="content-header">
  <h1>
    New Student
  </h1>
  <ol class="breadcrumb">
    <li><a href="/students"><i class="fa fa-dashboard"></i> Students</a></li>
    <li class="active">Index</li>
  </ol>
</section>

<!-- Main content -->
<section class="content">

  <div class="box">
    <div class="box-header">
      <div class="box-title">Create New Student</div>
      <div class="box-tools">
				<%= link_to 'Back', students_path, class: "btn btn-primary" %>
      </div>
    </div>
    <div class="box-body">
      <%= render 'form', student: @student %>
    </div>
  </div>
</section>
